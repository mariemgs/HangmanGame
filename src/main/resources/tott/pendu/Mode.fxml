<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tott.pendu.ModeController">
    <children>
        <Label layoutX="270.0" layoutY="75.0" prefHeight="56.0" prefWidth="259.0" text="Choose Mode">
            <font>
                <Font name="Bookman Old Style Bold" size="36.0" />
            </font>
        </Label>
        <Button layoutX="235.0" layoutY="200.0" mnemonicParsing="false" onAction="#chooseEasyMode" prefHeight="102.0" prefWidth="322.0" text="Easy Mode" textFill="#0b9424">
            <font>
                <Font name="System Bold" size="23.0" />
            </font>
        </Button>
        <Button layoutX="235.0" layoutY="325.0" mnemonicParsing="false" onAction="#chooseHardMode" prefHeight="108.0" prefWidth="316.0" text="Hard Mode" textFill="#e70808">
            <font>
                <Font name="System Bold" size="23.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
