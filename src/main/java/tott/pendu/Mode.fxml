<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tott.pendu.ModeController">
    <children>
        <Label layoutX="270.0" layoutY="75.0" prefHeight="56.0" prefWidth="259.0" text="Choose Mode">
            <font>
                <Font name="Bookman Old Style Bold" size="36.0" />
            </font>
        </Label>
        <Button layoutX="294.0" layoutY="167.0" mnemonicParsing="false" onAction="#chooseEasyMode" prefHeight="56.0" prefWidth="212.0" text="Easy Mode" textFill="#0b9424">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Button>
        <Button layoutX="294.0" layoutY="237.0" mnemonicParsing="false" onAction="#chooseHardMode" prefHeight="56.0" prefWidth="212.0" text="Hard Mode" textFill="#e70808">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Button>
        <Button fx:id="checkTreeButton" layoutX="290.0" layoutY="317.0" mnemonicParsing="false" onAction="#checkBinaryTree" prefHeight="56.0" prefWidth="219.0" text="Check Binary Tree">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
